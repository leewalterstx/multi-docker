{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [{
        "name": "client",
        "image": "leewalterstx/multi-client",
        "hostname": "client",
        "essential": false,
        "memory": 128
    },{ 
        "name": "server",
        "image": "leewalterstx/multi-server",
        "hostname": "api",
        "essential": false,
        "memory": 128
    },{ 
        "name": "worker",
        "image": "leewalterstx/multi-worker",
        "hostname": "worker",
        "essential": false,
        "memory": 128
    },{ 
        "name": "nginx",
        "image": "leewalterstx/multi-nginx",
        "essential": true,
        "memory": 128,
        "portMappings": [{
            "hostPort": 80,
            "containerPort": 80
        }],
        "links": ["client", "server"]
    }]
}